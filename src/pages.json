			{
				"path": "order/evaluate/evaluate",
				"style": {
					"navigationBarTitleText": "商品评价"
				}
			},
			{
				"path": "order/wxPay",
				"style": {
					"navigationBarTitleText": "订单支付",
					"navigationStyle": "custom"
				}
			},
			{
				"path": "order/afterSaleList",
				"style": {
					"navigationBarTitleText": "售后列表"
				}
			},
			{
				"path": "order/afterType",
				"style": {
					"navigationBarTitleText": "选择售后类型"
				}
			},
			{
				"path": "order/afterSale",
				"style": {
					"navigationBarTitleText": "售后申请"
				}
			},
			{
				"path": "user/u-avatar-cropper/u-avatar-cropper",
				"style": {
					"navigationBarTitleText": "图片编辑"
				}
			}
		]
