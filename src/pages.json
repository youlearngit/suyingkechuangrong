					"navigationBarTitleText": "详情"
				}
			},
			{
				"path": "goods/goodsEvaluate",
				"style": {
					"enablePullDownRefresh": true,
					"navigationBarTitleText": "商品评价"
				}
			},
			{
				"path": "user/myAddress",
				"style": {
					"enablePullDownRefresh": true,
					"navigationBarTitleText": "收货地址"
				}
			},
			{
				"path": "user/myFoot",
