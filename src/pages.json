{
	"easycom": {
		"autoscan": true,
		"custom": {
			"^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue",
			"^u-(.*)": "uview-plus/components/u-$1/u-$1.vue"
		}
	},
	"pages": [{
			"path": "pages/tabbar/index/index",
			"style": {
				"navigationStyle": "custom",
				"navigationBarTitleText": "首页",
				"enablePullDownRefresh": true,
				"backgroundTextStyle": "dark"
			}
		},
		{
				"path": "pages/project/index/index",
				"style": {
					"navigationStyle": "default",
					"navigationBarTitleText": "",
					"enablePullDownRefresh": true,
					"backgroundTextStyle": "dark",
					"navigationBarBackgroundColor": "rgb(152,190,255)"
				}
			},
			{
				"path": "pages/project/detail/index",
				"style": {
					"navigationBarTitleText": "详细介绍"
				}
			},
			{
				"path": "pages/capitalDemand/index/index",
				"style": {
					"navigationBarTitleText": "资金需求"
				}
			},
		{
			"path": "pages/tabbar/category/category",
			"style": {
				"navigationBarTitleText": "分类"
			}
		},
		{
			"path": "pages/tabbar/cart/cart",
			"style": {
				"navigationBarTitleText": "购物车",
				"enablePullDownRefresh": true
			}
		},
		{
			"path": "pages/tabbar/user/user",
			"style": {
				"navigationBarTitleText": "我的",
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/tabbar/user/setting",
			"style": {
				"navigationBarTitleText": "设置"
			}
		},
		{
			"path": "pages/login/login",
			"style": {
				"navigationBarTitleText": "登录"
			}
		},
		{
			"path": "pages/home/classList",
			"style": {
				"navigationBarTitleText": "商品列表",
				"navigationStyle": "custom"
			}
		}, {
			"path": "pages/home/search",
			"style": {
				"navigationBarTitleText": "搜索"
			}
		}
	],
	"subPackages": [{
		"root": "pagesMall",
		"pages": [{
				"path": "user/myInfo",
				"style": {
					"enablePullDownRefresh": true,
					"navigationBarTitleText": "个人信息"
				}
			},
			{
				"path": "order/confirmOrder",
				"style": {
					"enablePullDownRefresh": false,
					"navigationBarTitleText": "确认订单"
				}
			},
			{
				"path": "goods/goodsDetails",
				"style": {
					"navigationBarTitleText": "商品详情"
				}
			},
			{
				"path": "goods/advertDetails",
