			}
		],

		"midButton": {
			"width": "120px",
			"height": "80px",
			"text": "需求发布",
			"iconPath": "/static/images/tabBar/xqfb.png",
			"iconWidth": "70px",
			"backgroundImage": ""
		}

	},
	"condition": {
		"current": 3,
		"list": [{
				"name": "登录页面",
				"path": "pages/login/login"
			},
			{
				"name": "我的",
				"path": "pages/tabbar/user/user"
			},
			{
				"name": "分类",
				"path": "pages/tabbar/category/category"
			},
			{
				"name": "商品列表",
				"path": "pages/home/classList"
			},
			{
				"name": "商品详情页面",
				"path": "pagesMall/goods/goodsDetails",
				"query": "goodsId=13"
			},
			{
				"name": "个人中心",
				"path": "pagesMall/user/myInfo"
			}
		]
	}
